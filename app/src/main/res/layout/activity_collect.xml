<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/d_55"
        android:background="@color/green"
        >
        <Button
            android:onClick="doBack"
            android:layout_marginLeft="@dimen/d_10"
            android:background="@null"
            android:drawablePadding="@dimen/d_5"
            android:id="@+id/bt_rendezvous_back"
            android:drawableLeft="@mipmap/arrow1"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:textColor="@color/white"
            android:textSize="20sp"
            android:text="@string/string_coolect"
            />
    </RelativeLayout>
    <com.pqc.jingdianyou.tabIndicator.SlidingTabLayout
        android:id="@+id/collect_slidingtab"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        >

    </com.pqc.jingdianyou.tabIndicator.SlidingTabLayout>
    <android.support.v4.view.ViewPager
        android:id="@+id/collect_page"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        />
</LinearLayout>